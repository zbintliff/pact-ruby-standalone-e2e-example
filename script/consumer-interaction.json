{
  "consumer": {
    "name": "consumer"
  },
  "provider": {
    "name": "provider"
  },
  "interactions": [
    {
      "description": "a request for all clusters",
      "providerState": "a cluster exists",
      "request": {
        "method": "GET",
        "path": "/clusters"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "data": [
            {
              "environment": "environmentName",
              "name": "clusterName",
              "namespace": "namespaceName",
              "region": "regionName",
              "teamList": [
                "teamName"
              ]
            }
          ]
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          },
          "$.body.data[*].teamList": {
            "min": 1
          },
          "$.body.data[*].teamList[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}
